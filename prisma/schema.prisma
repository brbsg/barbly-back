// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  code Int     @id @default(autoincrement())
  barcode String 
  status Status
  imported_t DateTime 
  url String
  product_name String
  quantity String
  categories String
  packaging String
  brands String
  image_url String
}

enum Status{
  draft
  imported
}
